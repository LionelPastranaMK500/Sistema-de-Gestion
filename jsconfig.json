{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@constants/*": [
                "src/constants/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@modules/*": [
                "src/modules/*"
            ],
            "@routes/*": [
                "src/routes/*"
            ],
            "@services/*": [
                "src/services/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@layouts/*": [
                "src/layouts/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ],
            "@stores/*": [
                "src/stores/*"
            ]
        },
        "target": "ES2020",
        "module": "ESNext",
        "moduleResolution": "Node",
        "jsx": "react-jsx",
        "checkJs": false
    },
    "include": [
        "src"
    ]
}